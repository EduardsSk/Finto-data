    skos:inScheme musa: ;
    skos:prefLabel "arabeskit"@fi,
        "arabesker"@sv ;
    skos:scopeNote "Koska termillä on myös yleinen merkitys (arabialainen ornamentti), sitä on syytä käyttää  musiikin indeksoinnissa vain, kun säveltäjä on antanut teokselle tällaisen nimen ja aina  yhdessä soittimen nimen kanssa, esimerkiksi  arabeskit : piano : Ranska."@fi,
        "Lyhyehkö pianokappale."@fi .

musa:p475 a skos:Concept ;
    dct:created "1996-02-07"^^xsd:date ;
    dct:modified "2014-05-02T15:19:01+03:00"^^xsd:dateTime ;
    skos:inScheme musa: ;
    skos:prefLabel "ariosot"@fi,
