musa:p580 a skos:Concept ;
    dct:created "1996-02-07"^^xsd:date ;
    dct:modified "2014-05-02T15:19:05+03:00"^^xsd:dateTime ;
    skos:altLabel "divertissementit"@fi ;
    skos:inScheme musa: ;
    skos:prefLabel "divertimentot"@fi,
