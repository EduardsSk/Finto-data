      muso-meta:prefLabel "punktmusik"@sv , "pistemusiikki"@fi ;
      muso-meta:semTag "pistemusiikki"^^xsd:string .

muso:p94
      rdf:type muso-meta:Concept ;
      rdfs:subClassOf yso:p1515 , muso:p963 ;
      muso-meta:note "Koska termiä on eri aikoina käytetty monenlaisista sävellyksistä, sitä on syytä käyttää  vain säveltäjän fantasioiksi nimeämien teosten yhteydessä."@fi , "\"Mielikuvituksellinen\", yleensä soittimille sävelletty vapaamuotoinen teostyyppi."@fi ;
      muso-meta:prefLabel "fantasier"@sv , "fantasiat"@fi ;
      muso-meta:semTag "fantasiat"^^xsd:string .

muso:p940
      rdf:type muso-meta:Concept ;
      rdfs:subClassOf yso:p5072 , muso:p527 ;
      muso-meta:prefLabel "popyhtyeet"@fi , "popband"@sv ;
