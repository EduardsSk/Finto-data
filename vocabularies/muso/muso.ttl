      muso-meta:prefLabel "opera"@sv , "ooppera"@fi ;
      muso-meta:semTag "ooppera"^^xsd:string .

muso:p416
      rdf:type muso-meta:Concept ;
      rdfs:subClassOf muso:p587 ;
      owl:equivalentClass yso:p20374 ;
