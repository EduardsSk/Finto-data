	<rdf:Description rdf:about="http://cv.iptc.org/newscodes/scene/010100">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">headshot</skos:prefLabel>
		<skos:definition xml:lang="en-GB">A head only view of a person (or animal/s) or persons as in a montage. </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/scene/"/><!--Broader Relationships--><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/scene/010200">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">half-length</skos:prefLabel>
		<skos:definition xml:lang="en-GB">A torso and head view of a person or persons. </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/scene/"/><!--Broader Relationships--><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/scene/010300">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">full-length</skos:prefLabel>
		<skos:definition xml:lang="en-GB">A view from head to toe of a person or persons </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/scene/"/><!--Broader Relationships--><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/scene/010400">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">profile</skos:prefLabel>
		<skos:definition xml:lang="en-GB">A view of a person from the side </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/scene/"/><!--Broader Relationships--><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/scene/010500">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">rear view</skos:prefLabel>
		<skos:definition xml:lang="en-GB">A view of a person or persons from the rear. </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/scene/"/><!--Broader Relationships--><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/scene/010600">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">single</skos:prefLabel>
		<skos:definition xml:lang="en-GB">A view of only one person, object or animal. </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/scene/"/><!--Broader Relationships--><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/scene/010700">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">couple</skos:prefLabel>
		<skos:definition xml:lang="en-GB">A view of two people who are in a personal relationship, for example engaged, married or in a romantic partnership. </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/scene/"/><!--Broader Relationships--><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/scene/010800">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">two</skos:prefLabel>
		<skos:definition xml:lang="en-GB">A view of two people </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/scene/"/><!--Broader Relationships--><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/scene/010900">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">group</skos:prefLabel>
		<skos:definition xml:lang="en-GB">A view of more than two people </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/scene/"/><!--Broader Relationships--><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/scene/011000">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">general view</skos:prefLabel>
		<skos:definition xml:lang="en-GB">An overall view of the subject and its surrounds </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/scene/"/><!--Broader Relationships--><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/scene/011100">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">panoramic view</skos:prefLabel>
		<skos:definition xml:lang="en-GB">A panoramic or wide angle view of a subject and its surrounds </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/scene/"/><!--Broader Relationships--><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/scene/011200">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">aerial view</skos:prefLabel>
		<skos:definition xml:lang="en-GB">A view taken from above </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/scene/"/><!--Broader Relationships--><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/scene/011300">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">under-water</skos:prefLabel>
		<skos:definition xml:lang="en-GB">A photo taken under water </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/scene/"/><!--Broader Relationships--><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/scene/011400">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">night scene</skos:prefLabel>
		<skos:definition xml:lang="en-GB">A photo taken during darkness </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/scene/"/><!--Broader Relationships--><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/scene/011500">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">satellite</skos:prefLabel>
		<skos:definition xml:lang="en-GB">A photo taken from a satellite in orbit </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/scene/"/><!--Broader Relationships--><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/scene/011600">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">exterior view</skos:prefLabel>
		<skos:definition xml:lang="en-GB">A photo that shows the exterior of a building or other object </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/scene/"/><!--Broader Relationships--><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/scene/011700">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">interior view</skos:prefLabel>
		<skos:definition xml:lang="en-GB">A scene or view of the interior of a building or other object </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/scene/"/><!--Broader Relationships--><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/scene/011800">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">close-up</skos:prefLabel>
		<skos:definition xml:lang="en-GB">A view of, or part of a person/object taken at close range in order to emphasize detail or accentuate mood. Macro photography. </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/scene/"/><!--Broader Relationships--><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/scene/011900">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">action</skos:prefLabel>
		<skos:definition xml:lang="en-GB">Subject in motion such as children jumping, horse running </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/scene/"/><!--Broader Relationships--><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/scene/012000">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">performing</skos:prefLabel>
		<skos:definition xml:lang="en-GB">Subject or subjects on a stage performing to an audience </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/scene/"/><!--Broader Relationships--><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/scene/012100">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">posing</skos:prefLabel>
		<skos:definition xml:lang="en-GB">Subject or subjects posing such as a "victory" pose or other stance that symbolizes leadership. </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/scene/"/><!--Broader Relationships--><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/scene/012200">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">symbolic</skos:prefLabel>
		<skos:definition xml:lang="en-GB">A posed picture symbolizing an event - two rings for marriage </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/scene/"/><!--Broader Relationships--><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/scene/012300">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">off-beat</skos:prefLabel>
		<skos:definition xml:lang="en-GB">An attractive, perhaps fun picture of everyday events - dog with sunglasses, people cooling off in the fountain </skos:definition>
		<skos:inScheme rdf:resource="http://cv.iptc.org/newscodes/scene/"/><!--Broader Relationships--><!--Narrower Relationships--><!--SKOS Mapping Relationships--></rdf:Description><rdf:Description rdf:about="http://cv.iptc.org/newscodes/scene/012400">
		<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<skos:prefLabel xml:lang="en-GB">movie scene</skos:prefLabel>
		<skos:definition xml:lang="en-GB">Photos taken during the shooting of a movie or TV production. </skos:definition>
