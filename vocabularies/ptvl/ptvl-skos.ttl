        "Ammattiliikenne tieliikenteessä"@fi,
        "Professionell vägtrafik"@sv .

ptvl:P16.7 a skos:Concept ;
    skos:broader ptvl:P16 ;
    skos:inScheme ptvl: ;
    skos:notation "P16.7" ;
