ptvl:P21 a skos:Concept ;
    skos:inScheme ptvl: ;
    skos:narrower ptvl:P21.1,
        ptvl:P21.2 ;
    skos:notation "P21" ;
    skos:note "This service class includes consumer protection and consumer rights from the perspective of trade in goods and services as well as insurance."@en,
        "Tässä palveluluokassa käsitellään kuluttajansuojaa ja kuluttajan oikeuksia tavara- ja palvelukaupan sekä vakuutusten kannalta."@fi ;
    skos:prefLabel "Consumer matters"@en,
        "Kuluttaja-asiat"@fi ;
    skos:topConceptOf ptvl: .

ptvl:TT2 a skos:Concept ;
    skos:inScheme ptvl:TT ;
    skos:narrower ptvl:TT2.1,
