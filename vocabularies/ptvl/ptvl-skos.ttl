ptvl:P18 a skos:Concept ;
    skos:inScheme ptvl: ;
    skos:narrower ptvl:P18.1,
        ptvl:P18.2 ;
    skos:notation "P18" ;
    skos:note "This service class contains different map services and geographic information offered as a service."@en,
        "Tässä palveluluokassa käsitellään erilaisia karttapalveluja sekä palveluna tarjottavia paikkatietoaineistoja."@fi ;
    skos:prefLabel "Maps"@en,
        "Kartat"@fi ;
