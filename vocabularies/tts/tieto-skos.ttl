    skos:inScheme tts: ;
    skos:prefLabel "data lifecycle"@en,
        "tiedon elinkaari"@fi,
        "informationens livscykel"@sv ;
    skos:related tts:t35,
        tts:t61,
        tts:t7 ;
    skos:topConceptOf tts: .

tts:t23 a skos:Concept ;
