tts:t51 a skos:Concept ;
    dc:source "Tietotermit" ;
    skos:altLabel "tiedon louhinta"@fi,
        "tiedon rikastaminen"@fi,
        "tiedonrikastus"@fi,
        "datautvinning"@sv ;
    skos:broader tts:t1 ;
    skos:definition "data acquisition from a large dataset that contains structured data with methods that can identify the connections between different data"@en,
        "tiedonhankinta laajasta, rakenteista dataa sisältävästä tietoaineistosta asioiden välisiä yhteyksiä tunnistavien menetelmien avulla"@fi,
        "informationsinsamling från ett större informationsmaterial med hjälp av metoder som identifierar saksamband"@sv ;
    skos:inScheme tts: ;
    skos:note "The data mining process can utilise statistical methods, for example."@en,
        "Tiedonlouhintaa voidaan tehdä esimerkiksi tilastollisin menetelmin."@fi,
        "Informationsutvinning kan göras till exempel med statistiska metoder."@sv ;
    skos:prefLabel "data mining"@en,
        "tiedonlouhinta"@fi,
        "informationsutvinning"@sv ;
    skos:related tts:t82,
