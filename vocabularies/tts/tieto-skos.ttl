    isothes:broaderPartitive tts:t56 ;
    skos:definition "metadata format that conforms to a specific set of rules"@en,
        "tiettyjen sääntöjen mukainen metatiedon esitysmuoto"@fi,
        "representation av metadata enligt vissa regler"@sv ;
    skos:hiddenLabel "code"@en,
        "koodi"@fi,
        "kod"@sv ;
    skos:inScheme tts: ;
    skos:narrower tts:t118 ;
    skos:note "The format of a piece of code can be selected based on its intended use."@en,
        "Koodin esitysmuoto voidaan valita käyttötarkoituksen mukaan."@fi,
        "Representationen av en kod kan väljas enligt ändamål."@sv ;
    skos:prefLabel "code (in a code set)"@en,
