    isothes:broaderPartitive tts:t7 ;
    skos:altLabel "document management"@en ;
    skos:definition "area of information management that focuses on records data"@en,
        "tiedonhallinnan osa-alue, jonka kohteena on asiakirjatieto"@fi,
        "delområde inom informationshantering som gäller dokumentinformation"@sv ;
    skos:inScheme tts: ;
